{
    "name": "krok/laravel-oas",
    "description": "Laravel OpenAPI Swagger",
    "keywords": [
        "laravel",
        "oas",
        "api",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander",
            "email": "system.bin@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "swagger-api/swagger-ui": "^5.19",
        "zircote/swagger-php": "^5.0"
    },
    "require-dev": {
        "krok/laravel-pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Krok\\Oas\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krok\\Oas\\OasServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "pint --config=vendor/krok/laravel-pint/src/pint.json",
        "cs": [
            "@pint"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
